ttps://images.unsplash.com/photo-1439853949127-fa647821eba0?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=987&q=80');\n      background-position: center center;\n      background-size: cover;\n      width: 50%;\n      overflow: hidden;\n      border-radius: $border-radius-default;\n    }\n  }\n}\n","styles/variables.scss":"$color-white-default: #fff;\n$color-black-default: #000;\n$color-gray-background: #eaeff2;\n$color-blue-logo: #4780ef;\n$color-gray-nav: #a5b2b9;\n\n$border-radius-default: 12px;\n","styles/components/form.scss":".form {\n  height: 60%;\n  padding: 36px;\n\n  form {\n    display: flex;\n    flex-direction: column;\n\n    .full-name-wrapper {\n      display: flex;\n      justify-content: space-between;\n\n      .first-name-wrapper,\n      .last-name-wrapper {\n        display: flex;\n        background-color: #f2f6fb;\n        border-radius: 16px;\n        padding: 8px 18px;\n        align-items: center;\n        justify-content: space-between;\n        width: 47%;\n\n        .first-name-content-form,\n        .last-name-content-form {\n          label {\n            font-size: 14px;\n            color: $color-gray-nav;\n            padding-left: 3px;\n          }\n\n          input {\n            font-weight: 600;\n            font-size: 18px;\n            width: 80%;\n            margin-top: 4px;\n            border: none;\n            background-color: transparent;\n            padding: 3px;\n\n            &:focus {\n              outline: 2px solid $color-blue-logo;\n              border-radius: 5px;\n              background-color: $color-white-default;\n            }\n          }\n        }\n      }\n    }\n    .email-wrapper,\n    .password-wrapper,\n    .button-wrapper {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 18px;\n      background-color: #f2f6fb;\n      border-radius: 16px;\n      padding: 8px 18px;\n      align-items: center;\n      width: 100%;\n      .email-content-form,\n      .password-content-form {\n        display: flex;\n        flex-direction: column;\n        width: 90%;\n\n        label {\n          font-size: 14px;\n          padding-left: 3px;\n          color: $color-gray-nav;\n        }\n\n        input {\n          font-weight: 600;\n          font-size: 18px;\n          width: 100%;\n          margin-top: 4px;\n          border: none;\n          background-color: transparent;\n          padding: 3px;\n\n          &:focus {\n            outline: 2px solid $color-blue-logo;\n            background-color: $color-white-default;\n            border-radius: 5px;\n          }\n        }\n      }\n    }\n\n    .button-wrapper {\n      background-color: white;\n      padding: 0;\n\n      .change-method-btn {\n        color: $color-gray-nav;\n        background-color: #f2f6fb;\n      }\n\n      .create-account-btn {\n        color: $color-white-default;\n        background-color: $color-blue-logo;\n      }\n\n      button {\n        width: 47%;\n        padding: 20px 16px;\n        border-radius: 25px;\n        border: none;\n\n        &:hover {\n          cursor: pointer;\n        }\n      }\n    }\n  }\n}\n","styles/components/navbar.scss":"nav {\n  display: flex;\n  justify-content: center;\n  padding: 28px 12px 28px 6px;\n\n  ul {\n    width: 100%;\n    display: flex;\n    justify-content: space-between;\n    list-style: none;\n    align-items: center;\n\n    .logo-wrapper {\n      display: flex;\n      align-items: center;\n\n      .point-blue {\n        color: $color-blue-logo;\n        font-weight: bold;\n      }\n    }\n\n    li {\n      .circle-blue {\n        height: 25px;\n        width: 25px;\n        border-radius: 12.5px;\n        background-color: $color-blue-logo;\n        margin-right: 8px;\n      }\n\n      a.logo {\n        color: $color-black-default;\n      }\n      a {\n        color: $color-gray-nav;\n      }\n    }\n  }\n}\n","styles/components/formHeader.scss":".form-header {\n  paddin